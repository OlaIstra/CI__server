{
    "name": "ci-server",
    "version": "1.0.0",
    "description": "CI-server",
    "scripts": {
        "server:dev": "cross-env TARGET=server webpack && node dist/server/server.js",
        "client:dev": "cross-env webpack-dev-server --host 0.0.0.0 --open",
        "start:dev": "cross-env NODE_ENV=development PORT=8080 nodemon",
        "start": "cross-env NODE_ENV=production webpack",
        "test:server": "jest --config='./jest-server.config.js'",
        "test:client": "jest --config='./jest-client.config.js'",
        "ts-check": "tsc --noEmit --project ./tsconfig.json",
        "eslint": "eslint src/**/* server/**/* shared/**/* --fix --no-error-on-unmatched-pattern",
        "prettier": "prettier --write '**/*.ts' '**/*.tsx'",
        "fixtures": "ts-node -r tsconfig-paths/register ./server/fixtures/utils.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OlaIstra/CI__server.git"
    },
