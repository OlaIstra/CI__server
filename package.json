{
    "name": "ci-server",
    "version": "1.0.0",
    "description": "CI-server",
    "scripts": {
        "start": "cross-env TARGET=client webpack && cross-env TARGET=server webpack && node dist/server/server.js",
        "server:dev": "cross-env TARGET=server webpack && node dist/server/server.js",
        "test": "jest",
        "dev": "cross-env webpack-dev-server --host 0.0.0.0 --open",
        "start:dev": "cross-env NODE_ENV=development PORT=8080 nodemon",
        "server": "nodemon -r tsconfig-paths/register server/index.ts",
        "build": "cross-env NODE_ENV=production webpack",
        "ts-check": "tsc --noEmit --project ./tsconfig.json",
        "eslint": "eslint server/**/* src/**/* --fix --no-error-on-unmatched-pattern",
        "prettier": "prettier --write '**/*.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OlaIstra/CI__server.git"
    },
    "keywords": [
        "js",
        "javascript",
        "ts",
        "typescript",
        "nodejs"
    ],
    "author": "Olga Ibragimova",
    "license": "ISC",
    "bugs": {
