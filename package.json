        "@typescript-eslint/parser": "^3.0.0",
        "babel-eslint": "^8.2.6",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
