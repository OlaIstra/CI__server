        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js, jsx, ts, tsx}": [
            "npm run ts-check",
            "npm run eslint",
            "npm run prettier"
        ]
